@using AdminYonetimliCV.Models.Siniflar
@model List<Anasayfa>
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kişisel Web sitesi</title>
    <link rel="stylesheet" href="~/KişiselWebSitesi/Style.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
</head>
<body>
    <div class="center">
        <div class="content">
            @foreach(var x in Model)
            {
            <h1>@x.isim</h1>
                <h2>@x.unvan </h2>
                <p>@x.acıklama</p>
                <h3>@x.iletisim</h3>
            }
        </div>
        <a href="/Admin/Index/" class="btn btn-primary">Admine Git</a>
    </div>
</body>
</html>
